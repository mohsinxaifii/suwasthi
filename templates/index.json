  \/\/ Check if the countdown element is in the viewport and start the animation\n  function checkCountdownInViewport() {\n    const elementTop = countdownSpan.getBoundingClientRect().top;\n    const viewportHeight = window.innerHeight || document.documentElement.clientHeight;\n  \n    isCountdownInViewport = elementTop < viewportHeight;\n  }\n  \n  \/\/ Initially, pause the countdown\n  countdownSpan.textContent = \"24998449\";\n  \n  \/\/ Check if the countdown is in the viewport when the page loads\n  checkCountdownInViewport();\n  \n  \/\/ Add a scroll event listener to trigger the countdown animation\n  window.addEventListener(\"scroll\", () => {\n    checkCountdownInViewport();\n    animateCountdown();\n  });\n  \n  \/\/ Function to scale and rotate the floating image on scroll\n  function scaleAndRotateImageOnScroll() {\n    const scrollY = window.scrollY || window.pageYOffset;\n    const elementTop = floatingImage.getBoundingClientRect().top;\n    const elementHeight = floatingImage.offsetHeight;\n    const viewportHeight = window.innerHeight || document.documentElement.clientHeight;\n  \n    if (elementTop < viewportHeight && elementTop + elementHeight > 0) {\n      \/\/ Calculate the scale factor and rotation based on scroll position\n      const scaleValue = 1 + 0.2 * (1 - elementTop \/ viewportHeight);\n      const rotateValue = 5 * (1 - elementTop \/ viewportHeight); \/\/ Adjust the rotation value as needed\n      floatingImage.style.transform = `scale(${scaleValue}) rotate(${rotateValue}deg)`;\n    }\n  }\n  \n  \/\/ Add a scroll event listener to trigger the image animation\n  window.addEventListener(\"scroll\", () => {\n    scaleAndRotateImageOnScroll();\n  });\n  \n<\/script>",
